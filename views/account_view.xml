<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <record model="ir.ui.view" id="view_account_journal_form_pos_sat">
            <field name="name">view_account_journal_form_pos_sat</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <data>
                    <field name="code" required="0" position="after">
                        <field name="requiere_resolucion"/>
                        <field name="resolucion_id"/>
                        <field name="ultimo_numero_factura" readonly="True"/>
                    </field>
                </data>
            </field>
        </record>

        <menuitem action="account.action_account_journal_form" id="pos_sat_journal_action" parent="point_of_sale.menu_point_config_product"/>
    </data>
</odoo>
